            android:layout_width="250dp"
            android:layout_height="80.7dp"
            android:id="@+id/imageView4"
            android:scaleType="center"
            android:src="@drawable/biankuang"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="70dp"
