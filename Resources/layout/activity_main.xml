            android:layout_width="match_parent"
            android:layout_height="24.4dp"
            android:layout_centerInParent="true"
            android:text="@string/Main1String"
            android:textColor="@android:color/black"
            android:textAlignment="center"
            android:textStyle="bold"
            android:textSize="16dp"
            android:typeface="normal"
            android:layout_marginTop="45dp"
            android:layout_marginLeft="0.0dp"
        />
        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="24.4dp"
            android:layout_centerInParent="true"
            android:text="@string/Main1String2"
            android:textColor="@android:color/black"
            android:textAlignment="center"
            android:textStyle="bold"
            android:textSize="16dp"
            android:typeface="normal"
            android:layout_marginTop="0.0dp"
            android:layout_marginLeft="0.0dp"
        />
        <TextView
            android:id="@+id/textView9"
            android:layout_width="88.0dp"
            android:layout_height="24.4dp"
            android:layout_centerInParent="true"
            android:text="@string/Main1String3"
            android:textColor="@android:color/black"
            android:textAlignment="center"
            android:textStyle="normal|italic"
            android:textSize="16dp"
            android:typeface="serif"
            android:layout_marginTop="6.6dp"
            android:layout_marginLeft="8.7dp"
        />
    </LinearLayout>
    <ImageView
