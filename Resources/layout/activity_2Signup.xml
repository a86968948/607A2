<?xml version="1.0" encoding="utf-8" ?> 
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/linearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minWidth="25px"
    android:minHeight="25px">
    <LinearLayout
        android:id="@+id/linearLayout2"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:minWidth="25px"
        android:minHeight="25px">
        <TextView
            android:layout_width="313.8dp"
            android:layout_height="48.4dp"
            android:layout_centerInParent="true"
            android:text="@string/app_text"
            android:textColor="#02005A"
            android:textAlignment="viewEnd"
            android:layout_marginTop="22.7dp"
            android:textStyle="bold"
            android:textSize="32dp"
            android:typeface="serif"
            />
            <ImageView
            android:src="@drawable/logo"
            android:layout_width="79.3dp"
            android:layout_height="82.2dp"
            android:layout_toLeftOf="@id/textView1"
            android:id="@+id/homebtn1"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="0dp"
            android:layout_centerHorizontal="true"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <ImageView
            android:src="@drawable/signup1"
            android:layout_width="360.4dp"
            android:layout_height="482.9dp"
            android:layout_toLeftOf="@id/textView1"
            android:id="@+id/imageView1"
            android:layout_marginTop="57.7dp"
            android:layout_marginLeft="17.0dp"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:src="@drawable/signup2"
            android:layout_width="299.6dp"
            android:layout_height="58.2dp"
            android:id="@+id/imageView2"
            android:layout_marginTop="-380dp"
            android:layout_marginLeft="45dp"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:src="@drawable/signup2"
            android:layout_width="299.6dp"
            android:layout_height="58.2dp"
            android:id="@+id/imageView3"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="45dp"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:src="@drawable/signup2"
            android:layout_width="299.6dp"
            android:layout_height="58.2dp"
            android:id="@+id/imageView4"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="45dp"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:src="@drawable/signup2"
            android:layout_width="299.6dp"
            android:layout_height="58.2dp"
            android:id="@+id/imageView5"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="45dp"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:src="@drawable/signup2"
            android:layout_width="299.6dp"
            android:layout_height="58.2dp"
            android:id="@+id/imageView6"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="45dp"
            android:layout_centerHorizontal="true"
            />
        <ImageView
            android:src="@drawable/signup3"
            android:layout_width="279.6dp"
            android:layout_height="66.9dp"
            android:id="@+id/Signupbtn"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="58.0dp"
            android:layout_centerHorizontal="true"
            android:clickable="true"
            android:contentDescription="description" 
            />
        <TextView
            android:id="@+id/textView2"
            android:layout_width="259.3dp"
            android:layout_height="58.5dp"
            android:layout_centerInParent="true"
            android:text="@string/Signup1"
            android:textColor="@android:color/black"
            android:textAlignment="textStart"
            android:textStyle="bold"
            android:textSize="32dp"
            android:typeface="sans"
            android:layout_marginTop="-420dp"
            android:layout_marginLeft="45.9dp"
        />
        <EditText
            android:id="@+id/UserName_EditText"
            android:layout_width="259.3dp"
            android:layout_height="40.0dp"
            android:layout_centerInParent="true"
            android:hint="@string/Signup2"
            android:textColor="@android:color/black"
            android:textAlignment="textStart"
            android:textStyle="normal"
            android:textSize="25dp"
            android:typeface="sans"
            android:layout_marginTop="15.1dp"
            android:layout_marginLeft="62.9dp"
            android:inputType="text" 
        />
        <EditText
            android:id="@+id/PassWord_EditText"
            android:layout_width="259.3dp"
            android:layout_height="40.0dp"
            android:layout_centerInParent="true"
            android:hint="@string/Signup3"
            android:textColor="@android:color/black"
            android:textAlignment="textStart"
            android:textStyle="normal"
            android:textSize="25dp"
            android:typeface="sans"
            android:layout_marginTop="15.1dp"
            android:layout_marginLeft="62.9dp"
            android:inputType="textPassword" 
        />
        <EditText
            android:id="@+id/Name_EditText"
            android:layout_width="259.3dp"
            android:layout_height="40.0dp"
            android:layout_centerInParent="true"
            android:hint="@string/Signup4"
            android:textColor="@android:color/black"
            android:textAlignment="textStart"
            android:textStyle="normal"
            android:textSize="25dp"
            android:typeface="sans"
            android:layout_marginTop="18.7dp"
            android:layout_marginLeft="62.9dp"
            android:inputType="text" 
        />
        <EditText
            android:id="@+id/Mobile_EditText"
            android:layout_width="259.3dp"
            android:layout_height="40.0dp"
            android:layout_centerInParent="true"
            android:hint="@string/Signup5"
            android:textColor="@android:color/black"
            android:textAlignment="textStart"
            android:textStyle="normal"
            android:textSize="25dp"
            android:typeface="sans"
            android:layout_marginTop="18.7dp"
            android:layout_marginLeft="62.9dp"
            android:inputType="text" 
        />
        <EditText
            android:id="@+id/Email_EditText"
            android:layout_width="259.3dp"
            android:layout_height="40.0dp"
            android:layout_centerInParent="true"
            android:hint="@string/Signup6"
            android:textColor="@android:color/black"
            android:textAlignment="textStart"
            android:textStyle="normal"
            android:textSize="25dp"
            android:typeface="sans"
            android:layout_marginTop="19.9dp"
            android:layout_marginLeft="62.9dp"
            android:inputType="textEmailAddress" 
        />
        <TextView
            android:id="@+id/textView8"
            android:layout_width="259.3dp"
            android:layout_height="40.0dp"
            android:layout_centerInParent="true"
            android:text="@string/Signup7"
            android:textColor="@android:color/white"
            android:textAlignment="center"
            android:textStyle="bold"
            android:textSize="25dp"
            android:typeface="sans"
            android:layout_marginTop="17.3dp"
            android:layout_marginLeft="62.9dp"
        />
        <TextView
            android:id="@+id/textView9"
            android:layout_width="305.5dp"
            android:layout_height="40.0dp"
            android:layout_centerInParent="true"
            android:text="@string/Signup8"
            android:textColor="@android:color/black"
            android:textAlignment="textEnd"
            android:textStyle="normal"
            android:textSize="20dp"
            android:typeface="sans"
            android:layout_marginTop="56.6dp"
            android:layout_marginLeft="72.7dp"
        />
    
    </LinearLayout>
    



</LinearLayout>

